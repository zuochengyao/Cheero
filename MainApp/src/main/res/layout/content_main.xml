<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".activity.base.MainActivity"
    tools:showIn="@layout/activity_main">

    <Button
        android:id="@+id/to_styled_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TO StyledActivity"/>

    <Button
        android:id="@+id/to_custom_view_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TO CustomViewActivity"/>

    <Button
        android:id="@+id/to_sections_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TO SectionsActivity"/>

    <Button
        android:id="@+id/to_anim_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TO AnimActivity"/>

    <Button
        android:id="@+id/to_dialog_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TO DialogActivity"/>

    <Button
        android:id="@+id/to_option_activity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TO OptionActivity"/>

</LinearLayout>
