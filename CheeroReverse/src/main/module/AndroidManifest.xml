<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.icheero.reverse"
          xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/cheero_app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".activity.ReverseActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.JniActivity"/>
        <activity android:name=".activity.DisposeSoActivity"/>
        <activity android:name=".activity.DisposeManifestActivity"/>
    </application>

</manifest>